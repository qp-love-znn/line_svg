#./clustalw/clustalw-2.1-linux-x86_64-libcppstatic/clustalw2 -INFILE=./five/all.pep.fa -OUTPUT=FASTA -OUTFILE=./five/clustalw.fa
./mega/MEGAX/megax/megacc -a ./mega/infer_NJ_protein.mao -d ./clustalw.fa -o ./five/NJ_nucleotide

